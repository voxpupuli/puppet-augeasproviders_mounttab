{
  "name": "puppet-augeasproviders_mounttab",
  "version": "2.1.1",
  "author": "Vox Pupuli",
  "summary": "Augeas-based mounttab provider for Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-augeasproviders_mounttab",
  "project_page": "https://github.com/voxpupuli/puppet-augeasproviders_mounttab",
  "issues_url": "https://github.com/voxpupuli/puppet-augeasproviders_mounttab/issues",
  "description": "This module provides a provider for the mounttab type using the Augeas configuration API library.",
  "dependencies": [
    {
      "name": "puppet/augeasproviders_core",
      "version_requirement": ">= 3.0.0 <4.0.0"
    },
    {
      "name": "puppetlabs/mount_providers",
      "version_requirement": ">= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04",
        "20.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 6.1.0 < 8.0.0" }
  ]
}
